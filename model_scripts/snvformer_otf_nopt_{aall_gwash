#!/usr/bin/bash
torchrun \
  --master_addr="localhost" \
  --master_port="12424" \
   --nnodes=1 \
   --nproc_per_node=4 \
train_classifier.py \
    --encoder-type="linformer" \
	--total-epochs=50 \
	--num-layers=4 \
	--batch-size=6 \
	--embed-dim=64 \
	--num-heads=4 \
    --seq-len=123  \
    --linformer-k=64 \
    --h5-file="/data/ukbb/net_input/tin_fixed_order.5" \
    --test-frac=0.3 \
	--devices=0,1,2,3 \
    --encoder-snapshot="none.pt" \
    --no-load-encoder \
    --snapshot-path=/data/ukbb/v2_snapshots/snvformer_otf_nopt_tinetal.pt \
    --warmup-batches=500 \
    --warmup-min-lr=1e-7 \
    --warmup-max-lr=1e-7 \
    --main-scheduler-step-size=1000 \
    --main-scheduler-gamma=1.00 \
    --report-on-batch=1000 \
    --no-gradscaler \
    --position-encoding="otf" \
    --snv-encoding="embedding" \
    --ignore-chrom \
    --pos-combine="cat" \
    --torch-sdp \
    --ffn-scale=1 \
    --dropout=0.05 \
    --snv-embed-size=8 \
    --pos-embed-size=56 \
    --ignore-class \
    --model-type="dual_output" \
    --target="gout" \
    --no-tf-init \